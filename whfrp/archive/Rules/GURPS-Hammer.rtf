\par \pard \widctlpar\brdrt\brdrs\brdrw45\brsp20 \brdrl\brdrs\brdrw45\brsp20 \brdrb\brdrth\brdrw45\brsp20 \brdrr\brdrth\brdrw45\brsp20 
